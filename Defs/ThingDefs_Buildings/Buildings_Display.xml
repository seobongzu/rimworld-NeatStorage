	<ThingDef ParentName="sbzDisplayShelfBase">
		<defName>sbz_TwoTierDisplayShelf</defName>
		<label>2-tier display shelf</label>
		<description>A 2-tier display shelf that can store up to 12 different items.</description>
		<graphicData>
			<texPath>Things/Building/Storage/TwoTierDisplayShelf/sbz_TwoTierDisplayShelf</texPath>
			<graphicClass>Graphic_Multi</graphicClass>
			<drawSize>(4,4)</drawSize>
			<drawRotated>false</drawRotated>
			<damageData>
				<cornerTL>Damage/Corner</cornerTL>
				<cornerTR>Damage/Corner</cornerTR>
				<cornerBL>Damage/Corner</cornerBL>
				<cornerBR>Damage/Corner</cornerBR>
			</damageData>
		</graphicData>
		<rotatable>true</rotatable>
		<defaultPlacingRot>South</defaultPlacingRot>
		<costStuffCount>40</costStuffCount>
		<uiIconScale>0.8</uiIconScale>
		<uiOrder>1000</uiOrder>
		<building>
			<preventDeteriorationOnTop>true</preventDeteriorationOnTop>
			<ignoreStoredThingsBeauty>true</ignoreStoredThingsBeauty>
			<maxItemsInCell>6</maxItemsInCell>
			<fixedStorageSettings>
				<filter>
					<disallowNotEverStorable>true</disallowNotEverStorable>
